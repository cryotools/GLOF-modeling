FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    note        "mesh decomposition control dictionary";
    location    "system";
    object      decomposeParDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 16;
//method           scotch;
method          hierarchical;

hierarchicalCoeffs
{
   n		(4 4 1);
   delta 	0.001;
   order	xyz;
}

